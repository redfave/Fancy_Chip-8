        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fancy_Chip_8"
        mc:Ignorable="d"
        Title="About" Height="150" Width="300"
        ResizeMode="NoResize" Topmost="True">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" Margin="0,10,0,0">
            <TextBlock HorizontalAlignment="Center" Text="Chip8 Emulator Fancy_Chip-8"/>
